{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "~navigation": ["src/navigation/index"],
      "~screens": ["src/screens/index"],
      "~constants": ["src/constants/index"],
      "~components": ["src/components/index"],
      "~assets": ["./assets/*"],
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
